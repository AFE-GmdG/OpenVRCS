; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="OpenVRCS"
config/description="C# Wrapper around the godot_openvr addon"
run/main_scene="res://addons/OpenVRCS/Demo/Main.tscn"
config/icon="res://addons/OpenVRCS/OpenVRCS.png"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/OpenVRCS/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/OpenVRCS/openvr.gdnlib" ]

[physics]

common/enable_pause_aware_picking=true
